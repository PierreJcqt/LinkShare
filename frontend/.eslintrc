{
    "extends": [ 
        "eslint:recommended", 
        "plugin:vue/recommended" 
    ], 
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
        "rules": {
            "indent": ["error", 4],
            "quotes": ["error", "double"],
            "semi": ["error", "always"],
            "vue/no-v-html": "off",
            "vue/max-attributes-per-line": ["error", {
            "singleline": 3,
            "multiline": {
            "max": 1,
            "allowFirstLine": false
        }
        }]
    }
}
